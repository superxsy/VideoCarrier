{
  "include": [
    "app"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*"
  ],
  "defineConstant": {
    "DEBUG": true
  },
  "stubPath": "typings",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.13",
  "pythonPlatform": "Windows",
  "executionEnvironments": [
    {
      "root": "app"
    }
  ],
  "typeCheckingMode": "basic",
  "reportCallIssue": "warning",
  "reportGeneralTypeIssues": "warning"
}